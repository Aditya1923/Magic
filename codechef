import java.util.*;
public class Magic{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int n,d=0,s=0;
System.out.println("Enter a number");
n=sc.nextInt();
s=n;
while(s>9){
n=s;
s=0;
while(n>0){
d=n%10;
s=s+d;
n=n/10;
}
}
if(s==1)
System.out.println("Magic number");
else
System.out.println("Not a Magic number");
}
}
